{
  "name": "blitz-mongodb-material-ui",
  "version": "1.0.0",
  "engines": {
    "node": "16.5.0",
    "npm": "7.x"
  },
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start -p $PORT",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@prisma/client": "2.28.0",
    "blitz": "0.38.6",
    "final-form": "4.20.2",
    "react": "alpha",
    "react-dom": "alpha",
    "react-final-form": "6.5.3",
    "zod": "3.7.1"
  },
  "devDependencies": {
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.16",
    "eslint": "7.32.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "prettier": "2.3.2",
    "prettier-plugin-prisma": "0.15.0",
    "pretty-quick": "3.1.1",
    "preview-email": "3.0.4",
    "prisma": "2.28.0",
    "typescript": "~4.3"
  },
  "private": true
}
